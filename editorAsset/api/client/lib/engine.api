    hitArea: HitArea | Rectangle;
    /**
     * 遮罩，可以设置一个对象(支持位图和矢量图)，根据对象形状进行遮罩显示
     * 遮罩对象坐标系是相对遮罩对象本身的，即以该对象的0,0点为准
     */
    mask: Sprite;
