     * @return 矩形区域
     */
    getBounds(): Rectangle { return null };
    /**
     * 获取本对象在自己坐标系的矩形显示区域，计算量较大，尽量少用
     * @return 矩形区域
     */
    getSelfBounds(): Rectangle { return null };
    /**
     * X轴缩放值
     */
    scaleX: number;
    /**
     * Y轴缩放值
     */
    scaleY: number;
    /**
     * 旋转角度
     */
    rotation: number;
    /**
     * 水平倾斜角度
     */
    skewX: number;
    /**
     * 垂直倾斜角度
     */
    skewY: number;
